{
  "name": "json-array-length",
  "displayName": "JSON Length",
  "description": "Displays the length of arrays and property counts of objects in JSON files",
  "version": "0.0.2",
  "publisher": "Kelok",
  "repository": {
    "type": "git",
    "url": "https://github.com/kelokchan/json-length-extension.git"
  },
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "json",
    "array",
    "length",
    "count",
    "size",
    "object",
    "properties"
  ],
  "bugs": {
    "url": "https://github.com/kelokchan/json-length-extension/issues"
  },
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "JSON Length",
      "properties": {
        "jsonLength.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable the extension"
        },
        "jsonLength.showArrayLength": {
          "type": "boolean",
          "default": true,
          "description": "Show item count for arrays"
        },
        "jsonLength.showObjectPropertyCount": {
          "type": "boolean",
          "default": true,
          "description": "Show property count for objects"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.96.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "jsonc-parser": "^3.2.0"
  }
}